<div class="container">
  <div class="row">

    <div class="col-md-6 col-md-offset-1 col-sm-6 col-sm-offset-7">
      <h1> プロフィール </h1>
    </div>

    <div class="col-md-6 col-md-offset-7 col-sm-6 col-sm-offset-7">
      <h1> つぶやき一覧 </h1>
      <p><%= notice %></p>

      <% @tubuyakis.each do |tubuyaki| %>
        <p> <%= tubuyaki.content %> </p>
        <%= link_to "編集",edit_tubuyaki_path(tubuyaki.id),class: 'btn btn-sm btn-success' %>
        <%= link_to "削除",tubuyaki_path(tubuyaki.id), method: :delete,class: 'btn btn-sm btn-danger' %>
      <% end %>
    </div>

  </div>
</div>
